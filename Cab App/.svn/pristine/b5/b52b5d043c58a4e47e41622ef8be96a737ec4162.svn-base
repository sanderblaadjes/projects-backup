*{
	margin:0;
	padding:0;
	 -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
body{
	font-size:14px;
	color:#1e1e1c;
	font-family:Helvetica Neue Light, Helvetica, sans-serif;
	font-weight:normal; line-height:normal;
}
.header{
	font-size:20px;
	background:#272725;
	border-bottom:1px solid #1e1e1c;
	color:#fff;
	padding:10px 15px;
	font-weight:normal;
}
h1{
	font-size:20px;
	color:#1e1e1c;
	padding:15px 15px;
	font-weight:normal;
}
h2{
	font-size:16px;
	color:#1e1e1c;
	padding:15px 15px;
	font-weight:normal;
}
ul{
	padding:0 30px;
}
ol{
	padding:0 35px;
}
li {padding:5px 0; line-height:20px;}

p{
	line-height:20px;
	padding:12px 15px;
}
a{
	color:#da6307;
	font-weight:bold;
}
