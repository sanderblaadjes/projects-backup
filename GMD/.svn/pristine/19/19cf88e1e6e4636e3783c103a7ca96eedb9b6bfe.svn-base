                    android:singleLine="true"
                    android:text="@string/BACK_TO_TOP"
                    android:textColor="@color/service_checklist_text_color"
                    android:textSize="@dimen/normal_text_size" >
                </TextView>
            </LinearLayout>

            <ListView
                android:id="@+id/listview_services_secondgen"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:cacheColorHint="#00000000"
                android:choiceMode="singleChoice"
                android:listSelector="#00000000"
                android:paddingBottom="15dp"
                android:paddingLeft="30dp"
                android:paddingTop="15dp"
                android:visibility="gone" >
            </ListView>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_services_thirdgen"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:background="@drawable/service_checklist_list_bg"
            android:orientation="vertical"
            android:visibility="gone" >

            <LinearLayout
                android:id="@+id/ll_services_thirdgen_back"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="40dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/back_to_top" >

                <TextView
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginBottom="4dp"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="4dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/BACK_TO_TOP"
                    android:textColor="@color/service_checklist_text_color"
                    android:textSize="@dimen/normal_text_size" >
                </TextView>
            </LinearLayout>

            <ListView
                android:id="@+id/listview_servies_thirdgen"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:cacheColorHint="#00000000"
                android:choiceMode="singleChoice"
                android:listSelector="#00000000"
                android:paddingBottom="15dp"
                android:paddingLeft="30dp"
                android:paddingTop="15dp"
                android:visibility="gone" >
            </ListView>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_services_leaf"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:background="@drawable/service_checklist_list_bg"
            android:orientation="vertical"
            android:visibility="gone" >

            <LinearLayout
                android:id="@+id/ll_services_leaf_back"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="40dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/back_to_top" >

                <TextView
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginBottom="4dp"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="4dp"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/BACK_TO_TOP"
                    android:textColor="@color/service_checklist_text_color"
                    android:textSize="@dimen/normal_text_size" >
                </TextView>
            </LinearLayout>

            <ListView
                android:id="@+id/listview_servies_leaf"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:cacheColorHint="#00000000"
                android:choiceMode="singleChoice"
                android:listSelector="#00000000"
                android:paddingBottom="15dp"
                android:paddingLeft="30dp"
                android:paddingTop="15dp"
                android:visibility="gone" >
            </ListView>
        </LinearLayout>

        <!-- android:listSelector="@drawable/listview_background" -->
        <!-- <LinearLayout -->
        <!-- android:layout_width="match_parent" -->
        <!-- android:layout_height="wrap_content" -->
        <!-- android:layout_margin="2dp" -->
        <!-- android:layout_weight="1" -->
        <!-- android:background="#A3A3A3" > -->
        <!-- </LinearLayout> -->
